<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nantonelli.smu_now">

    <uses-permission android:name="android.permission.INTERNET" />
    <application android:allowBackup="true" android:label="@string/app_name"
        android:icon="@mipmap/icon2" android:theme="@style/AppTheme" android:name=".OasysApplication">

        <activity android:name=".Activity_Home"
            android:windowSoftInputMode="adjustPan"
            android:label="Oasys">
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity_Event"
            android:label="Event description page"
            android:parentActivityName=".Activity_Home">
        </activity>
    </application>

</manifest>
